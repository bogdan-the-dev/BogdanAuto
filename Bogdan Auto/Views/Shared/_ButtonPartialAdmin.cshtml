@model int
@*
    For more information on enabling MVC for empty projects, visit http://go.microsoft.com/fwlink/?LinkID=397860
*@
<td style="width: 150px">
    <div class="btn-group">
        <a asp-action="Disable"  class="btn btn-secondary" asp-route-id="@Model">
            <i class="fas fa-lock"></i>
        </a>
        <a asp-action="Enable" class="btn btn-success" asp-route-id="@Model">
            <i class="fas fa-lock-open"></i>
        </a>
        <a asp-action="Delete"  class="btn btn-danger" asp-route-id="@Model">
            <i class="fas fa-trash-alt"></i>
        </a>
    </div>
</td>
